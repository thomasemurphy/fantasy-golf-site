# Copy to .env and fill in values (never commit .env)

# Database (Heroku sets DATABASE_URL automatically)
# DATABASE_URL=postgres://user:pass@host/dbname

# Redis (Heroku Redis sets REDIS_URL automatically)
# REDIS_URL=redis://localhost:6379/0

# SportsDataIO API
SPORTSDATA_API_KEY=your_api_key_here

# Email
MAILER_FROM=noreply@feeleyturcup.com

# Sidekiq Web UI credentials
SIDEKIQ_USERNAME=admin
SIDEKIQ_PASSWORD=changeme

# Rails
RAILS_ENV=development
SECRET_KEY_BASE=run_rails_secret_to_generate
